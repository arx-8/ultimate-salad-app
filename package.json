{
  "name": "ultimate-salad-app",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": "^16",
    "npm": "0",
    "yarn": "^1"
  },
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "android": "expo start --android",
    "eject": "expo eject",
    "eslint-print-config": "eslint --print-config App.tsx > misc/eslint-print-config.json",
    "ios": "expo start --ios",
    "lint-dry:eslint": "eslint -c .eslintrc.js --max-warnings=0 .",
    "lint-dry:format": "prettier --check .",
    "lint-dry": "npm-run-all --parallel --continue-on-error lint-dry:* lint:tsc",
    "lint:eslint": "eslint -c .eslintrc.js --fix .",
    "lint:format": "prettier --write .",
    "lint:tsc": "tsc --noEmit",
    "lint": "npm-run-all --parallel --continue-on-error lint:*",
    "start": "expo start",
    "test-ci": "jest --ci",
    "test": "jest --watch",
    "update-all": "npx --yes npm-check-updates@13.0.3 -u --reject \"/^(@types/jest|@types/react-native|@types/react|jest|react|react-dom|react-native)$/\"",
    "web": "expo start --web"
  },
  "dependencies": {
    "@react-navigation/native": "6.0.10",
    "@react-navigation/native-stack": "6.6.2",
    "expo": "45.0.4",
    "expo-status-bar": "1.3.0",
    "native-base": "3.4.6",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-native": "0.68.2",
    "react-native-safe-area-context": "4.2.5",
    "react-native-screens": "3.13.1",
    "react-native-svg": "12.3.0",
    "react-native-web": "0.17.7",
    "utility-types": "3.10.0",
    "victory-native": "36.4.1"
  },
  "devDependencies": {
    "@babel/core": "7.18.2",
    "@react-native-community/eslint-config": "3.0.2",
    "@testing-library/react-native": "9.1.0",
    "@types/babel__core": "7.1.19",
    "@types/eslint": "8.4.2",
    "@types/jest": "26.0.14",
    "@types/prettier": "2.6.3",
    "@types/react": "17.0.45",
    "@types/react-native": "0.67.7",
    "@typescript-eslint/eslint-plugin": "5.27.0",
    "@typescript-eslint/parser": "5.27.0",
    "eslint": "8.16.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-config-standard": "17.0.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-n": "15.2.0",
    "eslint-plugin-promise": "6.0.0",
    "eslint-plugin-react": "7.30.0",
    "eslint-plugin-react-hooks": "4.5.0",
    "eslint-plugin-sort-destructure-keys": "1.4.0",
    "eslint-plugin-sort-keys-fix": "1.1.2",
    "eslint-plugin-typescript-sort-keys": "2.1.0",
    "jest": "26.6.3",
    "jest-expo": "45.0.1",
    "npm-run-all": "4.1.5",
    "prettier": "2.6.2",
    "prettier-plugin-organize-imports": "2.3.4",
    "tsd": "0.20.0",
    "typescript": "4.7.2"
  },
  "resolutions": {
    "@types/react": "17.0.39"
  }
